    <groundspeak:cache id="6407045" available="True" archived="False">
      <groundspeak:name>#02 H.A.B.T. - Il pozzo</groundspeak:name>
      <groundspeak:placed_by>jpavlik</groundspeak:placed_by>
      <groundspeak:owner>jpavlik</groundspeak:owner>
