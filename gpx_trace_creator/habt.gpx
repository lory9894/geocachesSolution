    <groundspeak:cache id="6407124" available="True" archived="False">
      <groundspeak:name>#06 H.A.B.T. - Stazione eliografica Punta Mezzodì</groundspeak:name>
      <groundspeak:placed_by>jpavlik</groundspeak:placed_by>
      <groundspeak:owner>jpavlik</groundspeak:owner>
